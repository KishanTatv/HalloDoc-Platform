<script asp-append-version="true">
    $(document).ready(function () {

        function hideOtherPortion() {
            const dashHead = document.querySelector('.dashHead');
            const dashs = document.querySelector('.DashSearch');
            dashHead.classList.add('d-none');
            dashs.classList.add('d-none');
        }

        $('.ViewCase').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/ViewCase',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    hideOtherPortion();
                    $("#partial").html(response);
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.ViewNote').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/ViewNotes',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    hideOtherPortion();
                    $("#partial").html(response);
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.BlockCase').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/PopupBlockcase',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    $(".PartialPopup").html(response);
                    var my = new bootstrap.Modal(document.getElementById('blockCase'));
                    my.show();
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.AssignCase').click(function () {
            var reqid = $(this).data('reqid');
            var phyCase = $(this).data('phycase');
            $.ajax({
                url: '/AdminDash/PopupAssigncase',
                method: 'POST',
                data: { reqid: reqid, phyCase: phyCase },
                success: function (response) {
                    $(".PartialPopup").html(response);
                    var my = new bootstrap.Modal(document.getElementById('assignCase'));
                    my.show();
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.CancelCase').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/PopupCancelcase',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    $(".PartialPopup").html(response);
                    var my = new bootstrap.Modal(document.getElementById('cancelCase'));
                    my.show();
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.ViewUpload').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/ViewUploads',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    hideOtherPortion();
                    $("#partial").html(response);
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
            });
        });

        $('.OrderCase').click(function () {
            var reqid = $(this).data('reqid');
            $.ajax({
                url: '/AdminDash/SendOrder',
                method: 'POST',
                data: { reqid: reqid },
                success: function (response) {
                    hideOtherPortion();
                    $("#partial").html(response);
                },
                error: function (xhr, status, error) {
                    console.error(xhr.responseText);
                }
