<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-between me-3 pb-2">

        <div>
            <span>
                Page : <span class="text-primary">
                    @(
                        ViewBag.CurrentPage + 1
                        )
                </span> / @ViewBag.TPage
            </span>
        </div>
        <div class="d-flex">
            @if (ViewBag.CurrentPage > 0)
            {
            <li>
                <div class="page-link text-warning" data-id="@ViewBag.dTable" data-page="@(ViewBag.CurrentPage - 1)">
                    Previous
                </div>
            </li>
            }
            @for (int i = 1; i <= ViewBag.TPage; i++)
            {
            <li class="page-item">
                <div class="page-link" data-id="@ViewBag.dTable" data-page="@(i - 1)">
                        @i
                </div>
            </li>
            }
            @if (ViewBag.TPage - 1 > ViewBag.CurrentPage)
            {
            <li>
                <div class="page-link text-warning" data-id="@ViewBag.dTable" data-page="@(ViewBag.CurrentPage + 1)">
                    Next
                </div>
            </li>
            }
        </div>

    </ul>
</nav>
